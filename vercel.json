{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/$1"
    },
    {
      "src": "/(.*).js",
      "dest": "/client/$1.js"
    },
    {
      "src": "/(.*).json",
      "dest": "/client/$1.json"
    },
    {
      "src": "/(.*).css",
      "dest": "/client/$1.css"
    },
    {
      "src": "/(.*).(png|jpg|jpeg|gif|svg)",
      "dest": "/client/$1.$2"
    },
    {
      "src": "/(.*).(woff|woff2|ttf|otf|eot)",
      "dest": "/client/$1.$2"
    }
  ]
}
