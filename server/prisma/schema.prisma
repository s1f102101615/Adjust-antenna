datasource db {
  provider = "postgresql"
  url      = env("API_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Task {
  id        String   @id
  label     String
  done      Boolean
  createdAt DateTime
}

model Calendar {
  id        String
  appoid    String   @id
  title     String
  details   String
  location  String
  startDate String
  startTime String
  endDate   String
  endTime   String
  createdAt DateTime
  group     String[]
}

model UserData {
  id         String   @id
  involvedid String[]
}
